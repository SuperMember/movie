/* pages/my/collection/index.wxss */
page{
  background: #f5f5f5;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
}

.nav_right{
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 100%;
  height: 100%;
  padding: 10px;
  background: #fff;
  box-sizing: border-box;
}
.nav_right .nav_right_items{
  float: left;
  width: 33.33%;
  height: 150px;
  text-align: center;
}
.nav_right .nav_right_items image{
  width: 60px;
  height: 80px;
}
.nav_right .nav_right_items text{
  display: block;
  margin-top: 5px;
  font-size: 10px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.clear{
  position:absolute;
  bottom: 0;
  left: 0;
  background: #00bebe;
  width: 100%;
}